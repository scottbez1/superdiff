<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:id="@+id/left_line_number"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        style="@style/DiffLineNumber"
        />
    <TextView
        android:id="@+id/left_line_contents"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        style="@style/DiffLine"
        />
    
    <TextView
        android:id="@+id/right_line_number"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        style="@style/DiffLineNumber"
        />
    <TextView
        android:id="@+id/right_line_contents"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        style="@style/DiffLine"
        />
</merge>
